Algoritmo "tabuada_enquanto"

Var

   n, i, resultado : inteiro

Inicio

      escreva("DIGITE O VALOR QUE VOCÊ DESEJA A TABUADA: ")
      leia(N)
      escreval
      
      i <- 1
      
      enquanto (i <= 10) faca
               resultado <- i*N
               escreval(i, " X ", N, " = ", resultado)
               i <- i +1
      
      fimenquanto


Fimalgoritmo